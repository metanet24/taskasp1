@model List<UserVM>



<table border="1">
    <thead>
        <tr>
            <th>FullName</th>
            <th>Age</th>
            <th>Email</th>
            <th>Adress</th>
        </tr>
    </thead>
    <tbody>
       
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.FullName</td>
                <td>@item.Age</td>
                <td>@item.Email</td>
                <td>@item.Adress</td>
            </tr>

        }
       
    </tbody>
</table>